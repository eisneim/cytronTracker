{
  "name": "cytrontracker",
  "version": "0.0.1",
  "description": "2d tracker for cytron project, this tracker use canvas and raw rgb data use techniques like after effects corner pin tracker",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.babel.js --debug --progress",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "vfx",
    "tracker"
  ],
  "babel": {
    "plugins": [
      [
        "csjs-postcss",
        {
          "plugins": [
            "autoprefixer"
          ]
        }
      ]
    ],
    "presets": [
      "es2015",
      "stage-1"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.7.2",
    "babel-eslint": "5.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-csjs-postcss": "^0.2.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-1": "^6.5.0",
    "chai": "^3.5.0",
    "csjs": "1.0.6",
    "eslint": "2.4.0",
    "eslint-loader": "1.3.0",
    "eslint-plugin-react": "4.2.3",
    "estraverse-fb": "^1.3.1",
    "exports-loader": "^0.6.3",
    "file-loader": "^0.9.0",
    "karma": "^0.13.22",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^1.0.0",
    "karma-mocha": "^0.2.2",
    "karma-mocha-reporter": "^2.0.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.5",
    "phantomjs-prebuilt": "^2.1.5",
    "url-loader": "^0.5.7",
    "webpack": "1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "debug": "2.2.0"
  },
  "author": "eisneim <eisneim1@gmail.com>",
  "license": "ISC"
}
